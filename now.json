{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@now/python"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "app.py"
        },
        {
	   "src": "/analogue",
           "dest": "app.py"
	},
        {
            "src": "/fnh",
            "status": 301,
            "headers": { "Location": "https://docs.google.com/spreadsheets/d/1oD72GWc1WLaUu7dFdFgKjCbl-zaIyxjA4yRj5llbPlM/edit#gid=0" }
        }
    ],
    "env": {
        "CLIENT_SECRET": "@algolia_read_token"
    }
}
